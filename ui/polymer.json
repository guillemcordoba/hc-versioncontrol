{
  "shell": "dist/app-shell.js",
  "entrypoint": "index.html",
  "fragments": ["dist/app-shell.js", "dist/branch-selector.js", "dist/note-component.js"],
  "npm": true,
  "moduleResolution": "node",
  "sources": ["dist/app-shell.js", "dist/branch-selector.js", "dist/note-component.js", "manifest/**", "manifest.json", "package.json"],
  "extraDependencies": [
    "robots.txt",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [{
    "bundle": true,
    "js": { 
      "minify": false,
      "compile": "es5",
      "transformModulesToAmd": true
    },
    "addServiceWorker": false,
    "addPushManifest": true
  }]
}
