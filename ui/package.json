{
  "name": "pwa-starter-kit",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "Polymer/pwa-starter-kit",
  "scripts": {
    "start": "gulp serve",
    "start:webpack": "concurrently \"tsc -w\" \"webpack-dev-server --open --mode development --hot\"",
    "build": "tsc && polymer build --auto-base-path && gulp prpl-server",
    "build:static": "tsc && polymer build",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@holochain/hc-redux-middleware": "^1.0.9",
    "@holochain/hc-web-client": "0.0.1",
    "@material/mwc-button": "^0.3.6",
    "@polymer/app-layout": "^3.0.0",
    "@polymer/marked-element": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@vaadin/vaadin-button": "^2.1.3",
    "@vaadin/vaadin-combo-box": "^4.2.5",
    "@vaadin/vaadin-context-menu": "^4.3.1",
    "@vaadin/vaadin-details": "^1.0.1",
    "@vaadin/vaadin-icons": "^4.2.1",
    "@vaadin/vaadin-item": "^2.1.0",
    "@vaadin/vaadin-list-box": "^1.1.0",
    "@vaadin/vaadin-progress-bar": "^1.1.0",
    "@vaadin/vaadin-select": "^2.0.3",
    "@vaadin/vaadin-tabs": "^2.1.1",
    "@vaadin/vaadin-text-field": "^2.3.2",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "gitgraph.js": "^1.15.1",
    "ipfs": "^0.34.4",
    "ipfs-postmsg-proxy": "^3.1.1",
    "lit-element": "^2.0.1",
    "lodash": "^4.17.11",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "vaadin-progress-bar": "^1.0.0-alpha2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.6.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.3",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.5.0",
    "typescript": "^3.1.3",
    "wct-mocha": "^1.0.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4",
    "workbox-webpack-plugin": "^3.6.3"
  }
}
